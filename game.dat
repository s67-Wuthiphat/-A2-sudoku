void setup() {
  size(900, 1000);
   
}

void draw() {
  background(255);
  strokeWeight(7);
  stroke(0);
  rect(0, 0, width, height-100);
  for (int i = 0; i <= 900; i += 100) {
    if( i==300 || i == 600 ){
      strokeWeight(4);
    }else {
    strokeWeight(1);
    }
    line(i, 0, i, 900);
    line(0, i, 900, i); 
  }
}
